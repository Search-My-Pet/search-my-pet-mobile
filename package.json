{
  "name": "search-my-pet",
  "version": "1.0.0",
  "main": "node_modules/expo/AppEntry.js",
  "scripts": {
    "start": "expo start",
    "start::no-cache": "expo start -c",
    "android": "yarn start::no-cache",
    "android::production": "yarn start::no-cache",
    "android::development": "yarn start::no-cache",
    "android::homolog": "yarn start::no-cache",
    "ios": "expo start --ios",
    "web": "expo start --web",
    "eject": "expo eject",
    "prepare": "husky install",
    "clear": "npx rimraf node_modules",
    "build::production::android": "eas build -p android --profile production",
    "build::development::android": "eas build -p android --profile development"
  },
  "dependencies": {
    "expo": "~45.0.0",
    "expo-status-bar": "~1.3.0",
    "react": "17.0.2",
    "react-dom": "17.0.2",
    "react-native": "0.68.2",
    "react-native-dotenv": "^3.3.1",
    "react-native-web": "0.17.7"
  },
  "devDependencies": {
    "@babel/core": "^7.12.9",
    "@commitlint/cli": "^17.0.3",
    "@commitlint/config-conventional": "^17.0.3",
    "@types/react": "~17.0.21",
    "@types/react-native": "~0.66.13",
    "@types/react-native-dotenv": "^0.2.0",
    "husky": "^8.0.0",
    "typescript": "~4.3.5"
  },
  "private": true
}